<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="18dp"
    android:layout_marginLeft="12dp"
    android:id="@+id/parent_constraint_layout"
    tools:context="com.rbricks.appsharing.concept.customcanvasdrawing.CustomDrawingActivity">

    <ImageView
        android:id="@+id/first_iv"
        android:layout_width="wrap_content"
        android:src="@drawable/cab_icon"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        />

    <ImageView
        android:id="@+id/second_iv"
        android:layout_width="wrap_content"
        android:src="@drawable/ic_launcher"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/first_iv"
        app:layout_constraintLeft_toLeftOf="@+id/first_iv"
        android:layout_height="wrap_content"/>

</android.support.constraint.ConstraintLayout>
